triggers='CREATE DEFINER=`root`@`localhost` TRIGGER antesInserirEstado BEFORE INSERT ON estado FOR EACH ROW\nBEGIN\nSET NEW.NOME = UPPER(NEW.NOME);  \nSET NEW.SIGLA = UPPER(NEW.SIGLA);  \nSET NEW.DTINSERT = CURRENT_DATE();  \nSET NEW.HRINSERT = CURRENT_TIME();  \nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER antesAlterarEstado BEFORE UPDATE ON estado FOR EACH ROW\nBEGIN\nSET NEW.NOME = UPPER(NEW.NOME);  \nSET NEW.SIGLA = UPPER(NEW.SIGLA);  \nSET NEW.DTUPDATE = CURRENT_DATE();  \nSET NEW.HRUPDATE = CURRENT_TIME();  \nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER antesExcluirEstado BEFORE DELETE ON estado FOR EACH ROW\nBEGIN\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER depoisInserirEstado AFTER INSERT ON estado FOR EACH ROW\nBEGIN\n\nINSERT INTO bkpestado(IDESTADO,SIGLA,NOME,DTINSERT,HRINSERT,DTUPDATE,HRUPDATE)\nVALUES (NEW.IDESTADO,\nNEW.SIGLA,\nNEW.NOME,\nNEW.DTINSERT,\nNEW.HRINSERT,\nNEW.DTUPDATE,\nNEW.HRUPDATE);\n\nINSERT INTO historico (ACAO,TABELA,ID) VALUES ("INSERT","estado",NEW.IDESTADO);\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER depoisAlterarEstado AFTER UPDATE ON estado FOR EACH ROW\nBEGIN\n\nINSERT INTO bkpestado(IDESTADO,SIGLA,NOME,DTINSERT,HRINSERT,DTUPDATE,HRUPDATE)\nVALUES (NEW.IDESTADO,\nNEW.SIGLA,\nNEW.NOME,\nNEW.DTINSERT,\nNEW.HRINSERT,\nNEW.DTUPDATE,\nNEW.HRUPDATE);\n\nINSERT INTO historico (ACAO,TABELA,ID) VALUES ("UPDATE","estado",NEW.IDESTADO);\nEND' 'CREATE DEFINER=`root`@`localhost` TRIGGER depoisExcluirEstado AFTER DELETE ON estado FOR EACH ROW\nBEGIN\n\nINSERT INTO bkpestado(IDESTADO,SIGLA,NOME,DTINSERT,HRINSERT,DTUPDATE,HRUPDATE)\nVALUES (OLD.IDESTADO,\nOLD.SIGLA,\nOLD.NOME,\nOLD.DTINSERT,\nOLD.HRINSERT,\nOLD.DTUPDATE,\nOLD.HRUPDATE);\n\nINSERT INTO historico (ACAO,TABELA,ID) VALUES ("DELETE","estado",OLD.IDESTADO);\nEND'
